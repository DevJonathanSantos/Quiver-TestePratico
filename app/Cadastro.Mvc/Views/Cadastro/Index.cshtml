@model Contato
<form asp-action="Incluir">
    <div class="row">
        <div class="col-md-6">
            <div class="form-group">
                <label for="Nome">Nome:</label>
                <input asp-for="Nome" class="form-control" placeholder="Ex.: João" maxlength="150">
            </div>
        </div>
        <div class="col-md-6">
            <div class="form-group">
                <label for="Sobrenome">Sobrenome:</label>
                <input asp-for="Cpf" class="form-control" placeholder="Ex.: 999.999.999-99" maxlength="14">
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-6">
            <div class="form-group">
                <label for="Cep">Cep:</label>
                <input asp-for="Cep" class="form-control" placeholder="Ex.: 01011-100" maxlength="10">
            </div>
        </div>
        <div class="col-md-6">
            <div class="form-group">
                <label for="Telefone">Telefone:</label>
                <input asp-for="Telefone" class="form-control" placeholder="Ex.: (11) 1234-5678" maxlength="50">
            </div>
        </div>
    </div>
        <div class="form-group">
            <input type="submit" value="Create" class="btn btn-primary" />
        </div>
</form>
