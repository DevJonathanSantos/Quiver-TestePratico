@model Cadastro.Repositories.Models.ContatoTelefone
<form asp-action="Incluir">
    <div id="interface">
        <div class="row">
            <div class="col-md-6">
                <div class="form-group">
                    <label for="Nome">Nome:</label>
                    <input asp-for="Nome" class="form-control" placeholder="Ex.: João" maxlength="50">
                    <span asp-validation-for="Nome" class="text-danger"></span>

                </div>
            </div>
            <div class="col-md-5">
                <div class="form-group">
                    <label for="Cpf">Cpf:</label>
                    <input asp-for="Cpf" class="form-control" placeholder="Ex.: 999.999.999-99" maxlength="14">
                    <span asp-validation-for="Cpf" class="text-danger"></span>

                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-6">
                <div class="form-group">
                    <label for="Cep">Cep:</label>
                    <input asp-for="Cep" class="form-control" placeholder="Ex.: 01011-100" maxlength="9">
                    <span asp-validation-for="Cep" class="text-danger"></span>
                </div>
            </div>
            <div class="col-md-5">
                <label for="Telefone">Telefone:</label>
                <input asp-for="Telefone" class="form-control" placeholder="Ex.: (11) 95860-7051" maxlength="15">
                <span asp-validation-for="Telefone" class="text-danger"></span>
                <label for="Telefone"></label>
                <input asp-for="Telefone" class="form-control" placeholder="Ex.: (11) 95860-7051" maxlength="15">
                <span asp-validation-for="Telefone" class="text-danger"></span>
                <label for="Telefone"></label>
                <input asp-for="Telefone" class="form-control" placeholder="Ex.: (11) 95860-7051" maxlength="15">
                <span asp-validation-for="Telefone" class="text-danger"></span>
                <label for="Telefone"></label>
                <input asp-for="Telefone" class="form-control" placeholder="Ex.: (11) 95860-7051" maxlength="15">
                <label for="Telefone"></label><input asp-for="Telefone" class="form-control" placeholder="Ex.: (11) 95860-7051" maxlength="15">
                <span asp-validation-for="Telefone" class="text-danger"></span>
            </div>
        </div>
        <div id="image">
            <img src="~/Images/image.png" />
        </div>
        <div id="create">
            <div class="form-group">
                <input type="submit" value="Cadastrar" class="btn btn-primary" />
            </div>
        </div>
    </div>
</form>